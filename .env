# Environment Configuration
# API Configuration - Backend is running on port 9000
REACT_APP_API_URL=http://localhost:9000/api
REACT_APP_API_VERSION=v1

# Application Environment
REACT_APP_ENVIRONMENT=development

# Authentication
REACT_APP_JWT_SECRET=your-jwt-secret-key
REACT_APP_SESSION_TIMEOUT=3600

# AI Services
REACT_APP_AI_SERVICE_URL=http://localhost:5000
REACT_APP_OCR_API_KEY=your-ocr-api-key
REACT_APP_AI_MODEL_VERSION=v2.1

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf
REACT_APP_UPLOAD_ENDPOINT=/upload

# Database (if needed for client-side configuration)
REACT_APP_DB_NAME=edutrack_ai

# External Services
REACT_APP_ANALYTICS_ID=your-analytics-id
REACT_APP_ERROR_TRACKING_DSN=your-error-tracking-dsn

# Feature Flags
REACT_APP_ENABLE_AI_CHAT=true
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_BETA_FEATURES=false

# Development
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info

# Deployment
REACT_APP_BUILD_VERSION=1.0.0
REACT_APP_DEPLOYMENT_DATE=2024-01-01